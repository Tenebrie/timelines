{
	"name": "styx-frontend",
	"version": "0.1.0",
	"private": true,
	"homepage": "https://timelines.tenebrie.com",
	"scripts": {
		"analyze": "source-map-explorer 'build/assets/*.js'",
		"build": "tsc && vite build",
		"build:ci": "vite build",
		"preview": "vite preview",
		"lint": "eslint src/",
		"lint-fix": "eslint src/ --fix",
		"openapi": "npx @rtk-query/codegen-openapi ./openapi/openapi-config.js && yarn eslint src/api --fix",
		"prune-build-modules": "yarn install --production --ignore-scripts --prefer-offline",
		"start": "vite --host --clearScreen false",
		"test": "vitest",
		"prod": "serve -s build -l 8080"
	},
	"lint-staged": {
		"*.(ts|tsx)": "eslint --fix"
	},
	"dependencies": {
		"@rollbar/react": "^0.11.1",
		"@tiptap/extension-image": "^2.10.3",
