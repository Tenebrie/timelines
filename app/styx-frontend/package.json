{
	"name": "styx-frontend",
	"version": "0.1.0",
	"private": true,
	"type": "module",
	"homepage": "https://timelines.tenebrie.com",
	"scripts": {
		"analyze": "source-map-explorer 'build/assets/*.js'",
		"tsc": "tsc",
		"build": "tsc && vite build",
		"build:ci": "vite build",
		"lint": "eslint src/",
		"lint-fix": "eslint src/ --fix",
		"openapi": "npx @rtk-query/codegen-openapi ./openapi/openapi-config.cjs && yarn eslint src/api --fix",
		"prune-build-modules": "yarn install --production --ignore-scripts --prefer-offline",
		"start": "vite --host --clearScreen false",
		"preview": "vite build && vite preview",
		"test": "vitest",
		"prod": "serve -s build -l 8080"
	},
	"lint-staged": {
		"*.(ts|tsx)": "eslint --fix"
	},
	"dependencies": {
		"@tanstack/react-form": "^1.1.2",
		"@tanstack/react-router": "^1.102.1",
