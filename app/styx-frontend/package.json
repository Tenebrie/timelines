		"@testing-library/jest-dom": "^6.6.3",
		"@testing-library/react": "^16.1.0",
		"@testing-library/user-event": "^14.4.3",
		"@types/clone-deep": "^4.0.4",
		"@types/jest": "^29.5.11",
		"@types/lodash.debounce": "^4.0.7",
		"@types/lodash.throttle": "^4.1.7",
		"@types/node": "^22.10.2",
