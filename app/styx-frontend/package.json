{
	"name": "styx-frontend",
	"version": "0.1.0",
	"private": true,
	"type": "module",
	"homepage": "https://timelines.tenebrie.com",
	"scripts": {
		"analyze": "source-map-explorer 'build/assets/*.js'",
		"tsc": "tsc --noEmit",
		"build": "tsc --noEmit && vite build",
		"build:ci": "vite build",
		"lint": "eslint",
		"lint:fix": "eslint --fix",
		"openapi": "npx @rtk-query/codegen-openapi ./openapi/openapi-config.cjs && yarn eslint src/api --fix",
		"prune-build-modules": "yarn install --production --ignore-scripts --prefer-offline",
		"start": "vite --host --clearScreen false",
		"preview": "vite build && vite preview",
		"test": "vitest --run",
		"test:watch": "vitest",
		"test:coverage": "vitest run --coverage",
		"prod": "serve -s build -l 8080"
	},
	"lint-staged": {
		"*.(ts|tsx)": "eslint --fix"
	},
	"dependencies": {
		"@mui/x-date-pickers": "^8.27.2",
		"@tanstack/react-form": "^1.27.7",
		"@tanstack/react-router": "^1.147.3",
